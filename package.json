{
  "name": "cache-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "description": "{{description}}",
  "keywords": [
    "npm-package-template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme {{module}} --section=API",
    "lint": "documentation lint {{module}}",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "documentation": "^8.0.2",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.0.1",
    "ava": "^0.25.0",
    "rollup": "^0.63.4",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-executable": "^1.2.0",
    "semantic-release": "^15.8.1",
    "travis-deploy-once": "^5.0.1",
    "rollup-plugin-istanbul": "^2.0.1"
  },
  "engines": {
    "node": ">=8.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/cache-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/cache-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/cache-repository-provider#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}