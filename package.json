{
  "name": "cache-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/provider.mjs",
  "exports": {
    ".": "./src/provider.mjs"
  },
  "description": "caches repository provider content",
  "keywords": [
    "OCI",
    "cache",
    "container",
    "test"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "bocf": "src/bocf-cli.mjs"
  },
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/module.mjs --section=API && documentation readme ./src/provider.mjs --section=API && documentation readme dist/bocf-cli.mjs --section=API",
    "lint": "documentation lint ./src/module.mjs && documentation lint ./src/provider.mjs && documentation lint dist/bocf-cli.mjs"
  },
  "dependencies": {
    "commander": "^6.0.0",
    "config-expander": "^11.0.13",
    "content-entry": "^2.3.3",
    "pump": "^3.0.0",
    "repository-provider": "^23.6.4",
    "tar-stream": "^2.1.2"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "c8": "^7.3.0",
    "documentation": "^13.0.2",
    "github-repository-provider": "^7.9.9",
    "kronos-endpoint": "^2.14.15",
    "leveldown": "^5.4.0",
    "levelup": "^4.3.1",
    "semantic-release": "^17.1.1"
  },
  "engines": {
    "node": ">=14.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/cache-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/cache-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/cache-repository-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
