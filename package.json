{
  "name": "cache-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/provider.mjs",
  "module": "src/provider.mjs",
  "description": "caches repository provider content",
  "keywords": [
    "cache"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/provider.mjs --section=API",
    "lint": "documentation lint src/provider.mjs",
    "posttest": "markdown-doctest && npm run prepare",
    "test": "ava"
  },
  "dependencies": {
    "content-entry": "^2.2.4",
    "repository-provider": "^16.10.3"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^7.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "github-repository-provider": "^2.1.3",
    "leveldown": "^5.4.0",
    "levelup": "^4.3.1",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^16.0.2"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/cache-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/cache-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/cache-repository-provider#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
